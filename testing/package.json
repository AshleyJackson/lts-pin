{
  "name": "modules2",
  "version": "0.1.0",
  "bin": {
    "modules2": "bin/modules2.js"
  },
  "scripts": {
    "build": "pnpm envts && cdk deploy --require-approval never",
    "watch": "tsc -w",
    "lint": "pnpx eslint ./lib/",
    "hotswap": "pnpm lint && cdk deploy --watch --hotswap --require-approval never",
    "envts": "doppler secrets download --no-file --format env > .env && pnpx gen-env-types .env",
    "cdk": "cdk",
    "deploying": "pnpm envts && cdk deploy --watch",
    "pr": "gh pr create -a @me -B v1 -H indev --fill-first && gh pr merge --auto -m",
    "livedev": "ngrok http http://localhost:3000",
    "home": "tsx --watch home.ts",
    "test": "tsx ./test.ts"
  },
  "devDependencies": {
    "@eslint/js": "~9.36.0",
    "@types/jest": "~29.5.14",
    "@types/node": "~22.18.6",
    "aws-cdk": "~2.1029.2",
    "eslint": "~8.57.1",
    "eslint-plugin-no-secrets": "~1.1.2",
    "globals": "~15.15.0",
    "jest": "~29.7.0",
    "ts-jest": "~28.0.8",
    "ts-node": "~9.1.1",
    "typescript": "~4.9.5",
    "typescript-eslint": "~7.18.0"
  },
  "dependencies": {
    "@aws-sdk/client-auto-scaling": "~3.893.0",
    "@aws-sdk/client-ec2": "~3.893.0",
    "@aws-sdk/client-rds": "~3.893.0",
    "@aws-sdk/client-ses": "~3.893.0",
    "@aws-sdk/client-ssm": "~3.893.0",
    "@fastify/formbody": "~7.4.0",
    "@octokit/rest": "~21.1.1",
    "@pagerduty/pdjs": "~2.1.0",
    "@sentry/serverless": "~6.19.7",
    "@slack/types": "~1.10.0",
    "@slack/web-api": "~6.13.0",
    "@types/aws-lambda": "~0.0.35",
    "@types/bun": "~1.1.18",
    "@types/luxon": "~2.4.0",
    "@types/luxon-business-days": "~3.0.0",
    "aws-cdk-lib": "~2.215.0",
    "aws-lambda": "~0.1.2",
    "axios": "~0.30.1",
    "cloudflare": "~4.5.0",
    "dotenv": "~16.6.1",
    "fastify": "~4.29.1",
    "get-root-path": "~2.0.2",
    "kysely": "~0.27.6",
    "luxon": "~2.5.2",
    "luxon-business-days": "~2.8.3",
    "mysql2": "~2.3.3",
    "nanoid": "~4.0.2",
    "octokit": "~4.1.4",
    "prisma": "~5.22.0",
    "prisma-kysely": "~1.8.0",
    "slack-block-builder": "~1.10.0",
    "tsx": "~3.14.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "aws-sdk",
      "esbuild",
      "prisma",
      "unrs-resolver"
    ]
  },
  "trustedDependencies": [
    "@prisma/engines",
    "@sentry/cli",
    "aws-sdk",
    "bun",
    "esbuild",
    "prisma",
    "unrs-resolver"
  ]
}